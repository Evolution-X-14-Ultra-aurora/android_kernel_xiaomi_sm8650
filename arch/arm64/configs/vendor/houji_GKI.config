# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_INPUT_FINGERPRINT=y

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# MTD
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m

# Power supply
CONFIG_MI_WIRELESS=y
CONFIG_MI_DTPT=y
CONFIG_MI_ENABLE_DP=y

# Product
CONFIG_TARGET_PRODUCT_HOUJI=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m
CONFIG_MI_THERMAL_MULTI_CHARGE=y

# Xiaomi Power
CONFIG_MI_POWER_INFO=m

# Xiaomi HWID
CONFIG_MI_HARDWARE_ID=m

# Xiaomi Wifi
CONFIG_MI_WIFI_GPIO=m

# Zram
CONFIG_ZRAM_WRITEBACK=y
